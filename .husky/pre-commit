npm run type-check
npm run lint
if command -v pipx > /dev/null 2>&1; then
  pipx run reuse download --all && pipx run reuse lint
else
  if command -v reuse > /dev/null 2>&1; then
    reuse download --all && reuse lint
  else
    echo "install reuse or pipx to enable license check"
    exit 1
  fi
fi
npm test
