{
  "name": "@zextras/zapp-shell",
  "version": "0.0.55",
  "description": "A web application framework",
  "scripts": {
    "test": "jest",
    "prepublishOnly": "npm run build:clean && npm run build",
    "postpublish": "rm -rf lib",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:clean": "rm -rf lib && rm -rf build && rm -rf pkg && rm -rf zapp-ui/node_modules && rm -rf zapp-ui/dist",
    "build": "webpack -c webpack.config.js",
    "build:types": "tsc --emitDeclarationOnly && tsc src/extension/shimmed/*.ts --outDir lib/",
    "build:zimlet": "npm run build:clean && npm run build && mkdir -p pkg && cd build && zip -r ../pkg/com_zextras_zapp_shell.zip .",
    "watch:docs": "cd docs/website/ && npm start || npm install && npm start",
    "deploy": "npm run prepublishOnly && rm -rf $PKG_PATH/node_modules/@zextras/zapp-shell/build/ && cp -r build/ $PKG_PATH/node_modules/@zextras/zapp-shell/"
  },
  "keywords": [
    "zapp",
    "zextras"
  ],
  "author": "Michele Olivo <michele.olivo@zextras.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.6.0",
    "@types/jest": "25.2.1",
    "@types/lodash": "4.14.144",
    "@types/react": "16.9.20",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "@types/webpack-env": "1.15.1",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "@zextras/zapp-cli": "0.0.28",
    "autoprefixer": "9.7.4",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "css-loader": "3.4.2",
    "eslint": "6.6.0",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "file-loader": "5.1.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "25.1.0",
    "jest-junit": "10.0.0",
    "mini-css-extract-plugin": "0.9.0",
    "postcss-loader": "3.0.0",
    "react-test-renderer": "16.13.1",
    "typescript": "3.6.4",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9"
  },
  "dependencies": {
    "@popperjs/core": "2.0.6",
    "@testing-library/react-hooks": "3.2.1",
    "@tinymce/tinymce-react": "3.5.0",
    "core-js": "3.6.4",
    "dexie": "2.0.4",
    "dexie-observable": "1.0.0-beta.6",
    "dexie-syncable": "1.0.0-beta.6",
    "i18next": "19.0.1",
    "i18next-browser-languagedetector": "4.0.1",
    "idb": "4.0.5",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
