{
  "name": "@zextras/zapp-shell",
  "version": "0.0.65",
  "description": "A web application framework",
  "types": "./types/index.d.ts",
  "scripts": {
    "start": "ZX_SHELL_FLAVOR=E2E webpack-dev-server --config webpack.e2e.config.js",
    "webdriver:start": "webdriver-manager update && webdriver-manager start",
    "test:e2e": "protractor protractor.conf.js",
    "test": "jest",
    "prepublishOnly": "npm run build:clean && ZX_SHELL_FLAVOR=NPM npm run build",
    "postpublish": "rm -rf lib",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:clean": "rm -rf lib && rm -rf build && rm -rf pkg && rm -rf zapp-ui/node_modules && rm -rf zapp-ui/dist",
    "build": "webpack -c webpack.config.js",
    "build:types": "tsc --emitDeclarationOnly && tsc src/extension/shimmed/*.ts --outDir lib/",
    "build:zimlet": "npm run build:clean && npm run build && mkdir -p pkg && cd build && zip -r ../pkg/com_zextras_zapp_shell.zip .",
    "watch:docs": "cd docs/website/ && npm start || npm install && npm start",
    "deploy": "ZX_SHELL_FLAVOR=NPM npm run prepublishOnly && rm -rf $PKG_PATH/node_modules/@zextras/zapp-shell/build/ && cp -r build/ $PKG_PATH/node_modules/@zextras/zapp-shell/build/ && cp -r types/ $PKG_PATH/node_modules/@zextras/zapp-shell/types/"
  },
  "keywords": [
    "zapp",
    "zextras"
  ],
  "author": "Michele Olivo <michele.olivo@zextras.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.6.0",
    "@types/jest": "25.2.1",
    "@types/lodash": "4.14.144",
    "@types/react": "16.9.20",
    "@types/react-dom": "16.9.5",
    "@types/react-router-dom": "5.1.3",
    "@types/semver": "7.2.0",
    "@types/webpack-env": "1.15.1",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "autoprefixer": "9.7.6",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-i18next-extract": "0.7.2",
    "babel-plugin-istanbul": "6.0.0",
    "babel-plugin-styled-components": "1.10.6",
