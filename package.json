{
  "name": "@zextras/zapp-shell",
  "version": "0.0.22",
  "description": "A web application framework",
  "scripts": {
    "prepublishOnly": "rm -rf lib && npx zapp package && npm run build:types && npx tsc src/extension/shimmed/*.ts --outDir lib/",
    "postpublish": "rm -rf lib",
    "watch:docs": "docsify serve ./docs",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:types": "npx tsc --emitDeclarationOnly"
  },
