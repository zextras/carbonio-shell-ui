{
  "name": "@zextras/zapp-shell",
  "version": "0.0.43",
  "description": "A web application framework",
  "scripts": {
    "prepublishOnly": "npm run build:clean && npm run build && npm run build:types",
    "postpublish": "rm -rf lib",
    "type-check": "npx tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build:clean": "rm -rf lib && rm -rf build && rm -rf pkg",
    "build": "webpack -c webpack.config.js",
    "build:types": "tsc --emitDeclarationOnly && tsc src/extension/shimmed/*.ts --outDir lib/",
    "build:zimlet": "npm run build:clean && npm run build && mkdir -p pkg && cd build && zip -r ../pkg/com_zextras_zapp_shell.zip .",
    "watch:docs": "cd docs/website/ && npm start || npm install && npm start"
  },
  "keywords": [
    "zapp",
    "zextras"
  ],
  "author": "Michele Olivo <michele.olivo@zextras.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.2",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.6.0",
    "@types/lodash": "4.14.144",
    "@types/react": "16.9.20",
    "@types/react-dom": "16.9.5",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "@zextras/zapp-cli": "0.0.28",
    "autoprefixer": "9.7.4",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "css-loader": "3.4.2",
    "eslint": "6.6.0",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.9.0",
    "postcss-loader": "3.0.0",
    "typescript": "3.6.4",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9"
  },
  "dependencies": {
    "@zextras/zapp-ui": "0.0.10",
    "i18next": "19.0.1",
    "i18next-browser-languagedetector": "4.0.1",
    "idb": "4.0.5",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-i18next": "11.2.2",
    "react-router-dom": "5.1.2",
    "react-virtuoso": "0.12.6",
    "rxjs": "6.5.3",
    "styled-components": "4.4.1",
    "systemjs": "6.1.4",
    "typeface-roboto": "0.0.75"
  }
}
